CREATE TABLE `dls` (
  `keynumber` int NOT NULL,
  `comment_id` int DEFAULT NULL,
  `match_id` int DEFAULT NULL,
  `match_name` varchar(1000) DEFAULT NULL,
  `home_team` varchar(1000) DEFAULT NULL,
  `away_team` varchar(1000) DEFAULT NULL,
  `current_innings` varchar(1000) DEFAULT NULL,
  `innings_id` int DEFAULT NULL,
  `overs` int DEFAULT NULL,
  `ball` int DEFAULT NULL,
  `runs` int DEFAULT NULL,
  `shortText` varchar(1000) DEFAULT NULL,
  `isBoundary` tinyint(1) DEFAULT NULL,
  `isWide` tinyint(1) DEFAULT NULL,
  `isNoball` tinyint(1) DEFAULT NULL,
  `batsman1_id` int DEFAULT NULL,
  `batsman1_name` varchar(1000) DEFAULT NULL,
  `batsman1_runs` int DEFAULT NULL,
  `batsman1_balls` int DEFAULT NULL,
  `bowler1_id` int DEFAULT NULL,
  `bowler1_name` varchar(1000) DEFAULT NULL,
  `bowler1_overs` float DEFAULT NULL,
  `bowler1_maidens` int DEFAULT NULL,
  `bowler1_runs` int DEFAULT NULL,
  `bowler1_wkts` int DEFAULT NULL,
  `batsman2_id` int DEFAULT NULL,
  `batsman2_name` varchar(1000) DEFAULT NULL,
  `batsman2_runs` int DEFAULT NULL,
  `batsman2_balls` int DEFAULT NULL,
  `bowler2_id` int DEFAULT NULL,
  `bowler2_name` varchar(1000) DEFAULT NULL,
  `bowler2_overs` float DEFAULT NULL,
  `bowler2_maidens` float DEFAULT NULL,
  `bowler2_runs` float DEFAULT NULL,
  `bowler2_wkts` float DEFAULT NULL,
  `wicket_id` float DEFAULT NULL,
  `wkt_batsman_name` varchar(1000) DEFAULT NULL,
  `wkt_bowler_name` varchar(1000) DEFAULT NULL,
  `wkt_batsman_runs` float DEFAULT NULL,
  `wkt_batsman_balls` float DEFAULT NULL,
  `wkt_text` varchar(1000) DEFAULT NULL,
  `isRetiredHurt` tinyint(1) DEFAULT NULL,
  `text` varchar(1000) DEFAULT NULL,
  `innings_2_target` int DEFAULT NULL,
  PRIMARY KEY (`keynumber`)
);
